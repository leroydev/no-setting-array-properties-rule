{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rulesDirectory": ["./dist/src"],
    "rules": {
        "max-classes-per-file": false,
        "no-string-literal": false,
        "no-console": false,
        "no-setting-array-properties": true
    },
    "exclude": ["rules"]
}